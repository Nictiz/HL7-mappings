	<xs:include schemaLocation="../coreschemas/datatypes.xsd"/>
	<xs:include schemaLocation="../coreschemas/voc.xsd"/>
	<xs:include schemaLocation="COCT_MT040203.xsd"/>
	<xs:element name="Message" type="MCCI_MT000300.Message"/>
	<xs:complexType name="MCCI_MT000300.Message">
		<xs:sequence>
			<xs:element name="id" type="II"/>
			<xs:element name="creationTime" type="TS"/>
			<xs:element name="versionCode" type="CS" minOccurs="1"/>
			<xs:element name="interactionId" type="II"/>
			<xs:element name="profileId" type="II" minOccurs="1" maxOccurs="unbounded"/>
			<xs:element name="processingCode" type="CS"/>
			<xs:element name="processingModeCode" type="CS"/>
			<xs:element name="acceptAckCode" type="CS"/>
			<xs:element name="acknowledgement" type="MCCI_MT000300.Acknowledgement" minOccurs="1"/>
			<xs:element name="attentionLine" type="MCCI_MT000300.AttentionLine" minOccurs="0"
				maxOccurs="unbounded"/>
			<xs:element name="receiver" type="MCCI_MT000300.Receiver"/>
			<xs:element name="respondTo" type="MCCI_MT000300.RespondTo" minOccurs="0"
				maxOccurs="unbounded"/>
			<xs:element name="sender" type="MCCI_MT000300.Sender"/>
			<xs:group ref="ControlAct"/>
			<!-- OZ Version -->
			<!-- <xs:element name="controlActProcess" type="MCCI_MT000300.ControlActProcess"/> -->
		</xs:sequence>
		<xs:attribute name="type" type="Classes" default="Message"/>
		<xs:attribute name="templateId" use="optional">
			<xs:simpleType>
				<xs:list itemType="oid"/>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="typeID" use="optional">
			<xs:simpleType>
				<xs:list itemType="oid"/>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="realmCode" use="optional">
			<xs:simpleType>
				<xs:list itemType="cs"/>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="nullFlavor" type="cs" use="optional"/>
	</xs:complexType>
	<xs:complexType name="MCCI_MT000300.Acknowledgement">
		<xs:sequence>
			<xs:element name="acknowledgementDetail" type="MCCI_MT000300.AcknowledgementDetail"
				minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="targetMessage" type="MCCI_MT000300.TargetMessage"/>
		</xs:sequence>
		<xs:attribute name="type" type="Classes" default="Acknowledgement"/>
		<xs:attribute name="typeCode" type="cs" use="required"/>
		<xs:attribute name="templateId" use="optional">
			<xs:simpleType>
				<xs:list itemType="oid"/>
