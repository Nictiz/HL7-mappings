		</xs:sequence>
		<xs:attribute name="type" type="Classes" default="Device"/>
		<xs:attribute name="classCode" type="EntityClass" use="optional" default="DEV"/>
		<xs:attribute name="determinerCode" type="EntityDeterminer" use="optional"
