<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!-- vanaf augustus2006-release: versionCode en profileId verplicht -->
<!-- 20070130 Verwijderd (in NL niet te gebruiken): /securityText, /attachmentText  /Acknowledgement/messageWaitingNumber, /Acknowledgement/messageWaitingPriorityCode-->
<xs:schema targetNamespace="urn:hl7-org:v3" xmlns="urn:hl7-org:v3"
