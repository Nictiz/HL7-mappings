	</xs:complexType>
	<xs:complexType name="MCCI_MT000300.TargetMessage">
		<xs:sequence>
			<xs:element name="id" type="II"/>
		</xs:sequence>
		<xs:attribute name="type" type="Classes" default="Message"/>
